{
  "name": "kad-company-client",
  "version": "1.0.0",
  "scripts": {
    "check": "npm run type-check && npm run lint",
    "dev": "npm run check && cross-env PORT=3000 NODE_ENV=development node server.js",
    "build": "next build",
    "start": "npm run check && cross-env PORT=3000 NODE_ENV=production node server.js",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext .ts --ext .tsx --ext .js",
    "pm2:start": "pm2 start server.js --name \"kad-company\""
  },
  "dependencies": {
    "@apollo/client": "^3.0.2",
    "antd": "^4.4.3",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "gsap": "^3.4.2",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^2.0.5",
    "typescript": "3.7.3",
    "webpack": "^4.43.0"
  },
  "license": "ISC",
  "author": "Dillon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DillonMemo/kad-company-client.git"
  },
  "bugs": {
    "url": "https://github.com/DillonMemo/kad-company-client/issues"
  },
